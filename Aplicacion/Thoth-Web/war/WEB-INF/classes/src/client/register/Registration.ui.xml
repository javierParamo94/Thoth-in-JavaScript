<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
		<center>
			<table>
				<th colspan="2">
					<img src="/images/LogoPrincipal.png" style="width:384px;height:133px; "
						align="right">
					</img>
				</th>
				<tr>
					<td>Full name</td>
					<td>
						<g:TextBox ui:field="fName"></g:TextBox>
					</td>
				</tr>

				<tr>
					<td>E-mail address </td>
					<td>
						<g:TextBox ui:field="email"></g:TextBox>
					</td>
				</tr>
				<tr>
					<td>Password</td>
					<td>
						<g:PasswordTextBox ui:field="password"></g:PasswordTextBox>
					</td>
				</tr>
				<tr>
					<td></td>
					<td>
						<g:Button ui:field="submit_button"></g:Button>
						<g:Anchor ui:field="login_link"></g:Anchor>
					</td>
				</tr>
				<td>
					<g:SimplePanel ui:field="message"></g:SimplePanel>
				</td>
			</table>
			<br />
			<br />
			<br />
		</center>
	</g:HTMLPanel>
</ui:UiBinder> 